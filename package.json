  "publishConfig": {
    "@risserlabs:registry": "https://gitlab.com/api/v4/projects/40483025/packages/npm/",
    "registry": "https://gitlab.com/api/v4/projects/40483025/packages/npm/"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "eslintIgnore": [
    "esm",
    "lib"
  ],
  "jest": {
    "testRegex": "((/(tests|__tests__)((/|/[^_.\n][^/\n]*(/[^_.\n][^/\n]+)*/)[^_.\n][^/\n]*))|((/|/.*/)(([^_.\n][^/\n]*.(spec|test))|(spec|test)))).[jt]sx?$",
    "testEnvironment": "node",
    "verbose": true,
    "testPathIgnorePatterns": [
      "<rootDir>/esm/",
      "<rootDir>/lib/"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/tests/_setup.ts"
    ]
  },
  "jestSonar": {
    "reportPath": "node_modules/.tmp/reports",
    "reportFile": "test-report.xml",
    "indent": 4
  },
  "packageManager": "yarn@3.2.4",
  "prettier": "eslint-config-alloy/.prettierrc.js"
}
