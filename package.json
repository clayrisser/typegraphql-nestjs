{
  "name": "@risserlabs/typegraphql-nestjs",
  "version": "0.5.5",
  "description": "Basic integration of TypeGraphQL in NestJS. Allows to use TypeGraphQL features while integrating with NestJS modules system and dependency injector.",
  "keywords": [
    "type-graphql",
    "nest",
    "nestjs",
    "graphql"
  ],
  "homepage": "https://gitlab.com/risserlabs/nestjs/typegraphql-nestjs",
  "bugs": {
    "url": "https://gitlab.com/risserlabs/nestjs/typegraphql-nestjs/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://gitlab.com/risserlabs/nestjs/typegraphql-nestjs.git"
  },
  "license": "MIT",
  "author": "MichaÅ‚ Lytek (https://github.com/MichalLytek)",
  "directories": {
    "example": "examples",
    "test": "tests"
  },
  "contributors": [
    {
      "name": "Clay Risser",
      "email": "email@clayrisser.com",
      "url": "https://clayrisser.com"
    }
  ],
  "main": "lib/index.js",
  "module": "esm/index.js",
  "types": "lib/index.d.ts",
  "source": "src/index.ts",
  "exports": {
    ".": {
      "import": "./esm/index.js",
      "require": "./lib/index.js"
    },
    "./axiosInherit": {
      "import": "./esm/axiosInherit.js",
      "require": "./lib/axiosInherit.js"
    }
  },
  "files": [
    "esm",
    "lib",
    "src"
  ],
  "scripts": {
    "build": "make -s build",
    "test": "make -s test"
  },
  "lint-staged": {
    "src/**/*.ts": [
      "prettier --write"
    ],
    "**/*.md": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@babel/runtime": "^7.19.4",
    "core-js": "^3.26.0"
  },
  "devDependencies": {
    "@apollo/federation": "^0.37.1",
    "@apollo/gateway": "^2.1.4",
    "@apollo/subgraph": "~0.5.1",
    "@babel/cli": "^7.19.3",
    "@babel/core": "^7.19.6",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-decorators": "^7.19.6",
    "@babel/plugin-transform-runtime": "^7.19.6",
